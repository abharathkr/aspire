<section class="section">
  <div class="container">
    <% @feedbacks.each do |f| %>
      <div class="container-fluid order">
        <div class="row">
          <div class="col-md-10 col-md-offset-1">
            <div class="well well-md a">
              <% @user = User.find_by(id: f.user_id) %>
              <% @email = Email.find_by(email: @user.email) %>
              <div class="row div-ques">
                <div class="col-md-2">1. <%= f.f1 %></div>
                <div class="col-md-6"><%= f.comment1 %></div>
              </div>
              <div class="row div-ques">
                <div class="col-md-2">2. <%= f.f2 %></div>
                <div class="col-md-6"><%= f.comment2 %></div>
              </div>
              <div class="row div-ques">
                <div class="col-md-2">3. <%= f.f3 %></div>
                <div class="col-md-6"><%= f.comment3 %></div>
              </div>
              <div class="row div-ques">
                <div class="col-md-2">4. <%= f.f4 %></div>
                <div class="col-md-6"><%= f.comment4 %></div>
              </div>
              <div class="row">
                <div class="col-md-8">5. <%= f.comment5 %></div>
              </div>
              <div class="row">
                <div class="col-md-8">6. <%= f.comment6 %></div>
              </div>
              <div class="row">
                <div class="col-md-8">7. <%= f.comment7 %></div>
              </div>
              <div class="row">
                <div class="col-md-8">8. <%= f.comment8 %></div>
              </div>
              <div class="row">
                <div class="col-md-2">Comment: </div>
                <div class="col-md-6"><%= f.comment %></div>
              </div><hr>
              <div class="row">
                <div class="col-md-2"><%= @email.user_name %></div>
                <div class="col-md-2"><%= @email.email %></div>
              </div><hr>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</section>