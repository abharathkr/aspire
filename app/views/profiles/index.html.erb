
<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Place</th>
      <th>Phonenumber</th>
      <th>Degree1percentage</th>
      <th>Degree1college</th>
      <th>Degree1Branch</th>
      <th>Degree2percentage</th>
      <th>Degree2college</th>
      <th>Degree2Branch</th>
      <th>Twelvepercentage</th>
      <th>Twelvecollege</th>
      <th>Twelve place</th>
      <th>Tenthpercentage</th>
      <th>Tenthcollege</th>
      <th>Tenth place</th>
      <th>Technicalskills</th>
      <th>Nontechnicalskills</th>
      <th>Analyticalskills</th>
      <th>Projects</th>
      <th>Coursestaken</th>
      <th>Goodat</th>
      <th>Dreamjob</th>
      <th>Awards</th>
      <th>Internship</th>
      <th>User Name</th>
      <th>User Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <% @user = User.find_by(id: profile.user_id) %>
        <% @email = Email.find_by(email: @user.email) %>
        <td><%= profile.place %></td>
        <td><%= profile.phonenumber %></td>
        <td><%= profile.degree1p %></td>
        <td><%= profile.degree1c %></td>
        <td><%= profile.degree1branch %></td>
        <td><%= profile.degree2p %></td>
        <td><%= profile.degree2c %></td>
        <td><%= profile.degree2branch %></td>
        <td><%= profile.twelvep %></td>
        <td><%= profile.twelvec %></td>
        <td><%= profile.twelveplace %></td>
        <td><%= profile.tenthp %></td>
        <td><%= profile.tenthc %></td>
        <td><%= profile.tenths %></td>
        <td><%= profile.technicalskills %></td>
        <td><%= profile.nontechnicalskills %></td>
        <td><%= profile.analyticalskills %></td>
        <td><%= profile.projects %></td>
        <td><%= profile.coursestaken %></td>
        <td><%= profile.goodat %></td>
        <td><%= profile.dreamjob %></td>
        <td><%= profile.awards %></td>
        <td><%= profile.internship %></td>
        <td><%= @email.user_name %></td>
        <td><%= @email.email %></td>
        <!--td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
